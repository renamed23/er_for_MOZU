      "offset": 1654,
      "index": 77,
      "value": [
        "",
        "/E"
      ]
    },
    {
      "op": "4A",
      "offset": 1656,
      "index": 78,
      "value": [
        "女",
        "/E"
      ]
    },
    {
      "op": "44",
      "offset": 1660,
      "index": 79,
      "value": [
        "「お金持ちなのねえ。",
        "/C"
      ]
    },
    {
      "op": "44",
      "offset": 1674,
      "index": 80,
      "value": [
        "　道理で高価そうな洋服だったわ」",
        "/E"
      ]
    },
    {
      "op": "4A",
      "offset": 1698,
      "index": 81,
      "value": [
        "",
        "/E"
      ]
    },
    {
      "op": "4A",
      "offset": 1700,
      "index": 82,
      "value": [
        "女",
        "/E"
      ]
    },
    {
      "op": "44",
      "offset": 1704,
      "index": 83,
      "value": [
