      "value": [
        "u8:10",
        "u8:134",
        "u8:0"
      ]
    },
    {
      "op": "4A",
      "offset": 735,
      "index": 41,
      "value": [
        "蘭子",
        "/E"
      ]
    },
    {
      "op": "44",
      "offset": 741,
      "index": 42,
      "value": [
        "「……ええっ……こ、これは……っ！？」",
        "/E"
      ]
    },
    {
      "op": "4A",
      "offset": 762,
      "index": 43,
      "value": [
        "",
        "/E"
      ]
    },
    {
      "op": "44",
      "offset": 764,
      "index": 44,
      "value": [
        "　蘭子が息を飲む気配がした。",
        "/E"
      ]
    },
