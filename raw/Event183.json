    {
      "op": "4A",
      "offset": 369,
      "index": 34,
      "value": [
        "環",
        "/E"
      ]
    },
    {
      "op": "44",
      "offset": 373,
      "index": 35,
      "value": [
        "「はい編集長、揃っていますわ」",
        "/E"
      ]
    },
    {
      "op": "4A",
      "offset": 394,
      "index": 36,
      "value": [
        "",
        "/E"
      ]
    },
    {
      "op": "44",
      "offset": 396,
      "index": 37,
      "value": [
        "　理知的な瞳を輝かせ、環が立ち上がった。",
        "/W"
      ]
    },
    {
      "op": "44",
      "offset": 426,
      "index": 38,
      "value": [
        "　仕立てのいい洋服にキチンとした髪のパーマネントは、いかにも仕事",
        "/C"
      ]
    },
    {
      "op": "44",
      "offset": 467,
      "index": 39,
      "value": [
        "のできる女といった感じだ。",
        "/E"
      ]
    },
    {
      "op": "24",
      "offset": 484,
      "index": 40,
      "value": [
        "u8:8",
        "u8:46",
        "u8:1"
      ]
    },
    {
      "op": "4A",
      "offset": 488,
      "index": 41,
      "value": [
        "環",
        "/E"
      ]
    },
    {
      "op": "44",
      "offset": 492,
      "index": 42,
      "value": [
        "「こちらが次号の分です。",
        "/C"
      ]
    },
    {
      "op": "44",
      "offset": 509,
      "index": 43,
      "value": [
        "　時間があったので、校正もすませておきました」",
        "/E"
      ]
    },
    {
      "op": "4A",
      "offset": 538,
      "index": 44,
      "value": [
        "",
        "/E"
      ]
    },
    {
      "op": "44",
      "offset": 540,
      "index": 45,
      "value": [
        "　てきぱきと説明する。",
        "/E"
      ]
    },
    {
      "op": "4A",
      "offset": 555,
      "index": 46,
      "value": [
        "編集長",
        "/E"
      ]
    },
    {
      "op": "44",
      "offset": 563,
      "index": 47,
      "value": [
        "「そうか、ありがとう。",
        "/C"
      ]
    },
    {
      "op": "44",
      "offset": 576,
      "index": 48,
      "value": [
        "　有能な職業婦人を部下に持って、私は幸せだよ」",
        "/E"
      ]
    },
    {
      "op": "4A",
      "offset": 612,
      "index": 49,
      "value": [
        "",
        "/E"
      ]
    },
    {
      "op": "2E",
