    },
    {
      "op": "3B",
      "offset": 1635,
      "index": 141,
      "value": [
        "u8:1",
        "u8:38",
        "u8:4",
        "u8:5"
      ]
    },
    {
      "op": "42",
      "offset": 1640,
      "index": 142,
      "value": []
