      ]
    },
    {
      "op": "4A",
      "offset": 245,
      "index": 20,
      "value": [
        "りんご",
        "/E"
      ]
    },
    {
      "op": "44",
      "offset": 250,
      "index": 21,
      "value": [
        "「しょ……しょうがないんだよう。",
        "/C"
      ]
    },
    {
      "op": "44",
      "offset": 268,
      "index": 22,
      "value": [
        "　小野寺の当主もタマキも、わたいが見えてないもん。",
        "/C"
      ]
    },
    {
      "op": "44",
      "offset": 301,
      "index": 23,
      "value": [
        "　わたいがいるって知らなきゃ、大切にしようがないじゃないか」",
        "/E"
      ]
    },
    {
      "op": "4A",
      "offset": 336,
      "index": 24,
      "value": [
        "",
        "/E"
      ]
    },
    {
      "op": "2E",
      "offset": 338,
      "index": 25,
      "value": [
        "u8:32",
        "u8:3",
        "u8:3",
        "u8:5"
      ]
    },
    {
      "op": "4A",
      "offset": 343,
      "index": 26,
      "value": [
        "N1",
        "/E"
      ]
    },
    {
      "op": "44",
      "offset": 346,
      "index": 27,
      "value": [
        "「見えてないだと？",
        "/C"
      ]
    },
    {
      "op": "44",
      "offset": 358,
      "index": 28,
      "value": [
        "　適当な事を言うな。",
        "/C"
      ]
    },
    {
      "op": "44",
      "offset": 374,
      "index": 29,
      "value": [
        "　俺には見えるし、こうして抱く事だってできる」",
        "/E"
      ]
    },
    {
      "op": "4A",
      "offset": 403,
      "index": 30,
      "value": [
        "",
        "/E"
      ]
    },
    {
