    {
      "op": "44",
      "offset": 8,
      "index": 2,
      "value": [
        "　血染めの下肢を放り出し、りんごは虚空を見ていた。",
        "/W"
      ]
    },
    {
      "op": "44",
      "offset": 44,
      "index": 3,
      "value": [
        "　その顔は涙でぐしゃぐしゃだ。",
        "/E"
      ]
    },
    {
      "op": "4A",
      "offset": 63,
      "index": 4,
      "value": [
        "N1",
        "/E"
      ]
    },
    {
      "op": "44",
      "offset": 66,
      "index": 5,
      "value": [
        "「痛かったのか」",
        "/E"
      ]
    },
    {
      "op": "4A",
      "offset": 77,
      "index": 6,
      "value": [
        "",
        "/E"
      ]
    },
    {
      "op": "44",
      "offset": 79,
      "index": 7,
      "value": [
        "　キッと俺をにらむ。",
        "/E"
      ]
    },
    {
      "op": "2E",
      "offset": 92,
      "index": 8,
      "value": [
        "u8:32",
        "u8:2",
        "u8:3",
        "u8:5"
      ]
    },
    {
      "op": "4A",
