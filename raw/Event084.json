  "opcodes": [
    {
      "op": "05",
      "offset": 0,
      "index": 0,
      "value": [
        "u16:197"
      ]
    },
    {
      "op": "44",
      "offset": 3,
      "index": 1,
      "value": [
        "　ガチャリ。",
        "/W"
      ]
    },
    {
      "op": "44",
      "offset": 11,
      "index": 2,
      "value": [
        "　突然の金属音に、二人で凍りついた。",
        "/W"
      ]
    },
    {
      "op": "44",
      "offset": 39,
      "index": 3,
      "value": [
        "　廊下に通じる扉の錠が回っている。",
        "/E"
      ]
    },
    {
      "op": "44",
      "offset": 64,
      "index": 4,
      "value": [
        "　誰かが外から鍵を開けたのだ。",
        "/E"
      ]
    },
    {
      "op": "44",
      "offset": 85,
      "index": 5,
      "value": [
        "　まずい。",
        "/W"
      ]
    },
    {
      "op": "44",
      "offset": 92,
      "index": 6,
      "value": [
        "　セシリアを引きずり、寄せられたカーテンの陰に隠れた。",
        "/W"
      ]
    },
    {
      "op": "44",
      "offset": 125,
      "index": 7,
      "value": [
        "　重くたっぷりした布地は、人間さえも隠してくれそうだった。",
        "/E"
      ]
    },
    {
      "op": "2F",
      "offset": 162,
      "index": 8,
      "value": [
        "u8:3",
        "u8:5"
      ]
    },
    {
      "op": "44",
      "offset": 165,
      "index": 9,
      "value": [
        "　間一髪。",
        "/W"
      ]
    },
    {
      "op": "44",
      "offset": 175,
      "index": 10,
      "value": [
        "　開いたドアから入ってきたのは、スチュワート氏だった。",
        "/W"
      ]
    },
    {
      "op": "44",
      "offset": 207,
      "index": 11,
      "value": [
        "　続いて、セシリアの婚約者のロバート。",
        "/E"
      ]
    },
    {
      "op": "2E",
      "offset": 232,
      "index": 12,
      "value": [
        "u8:15",
        "u8:1",
        "u8:2",
        "u8:5"
      ]
    },
    {
      "op": "4A",
      "offset": 237,
      "index": 13,
      "value": [
        "スチュワート",
        "/E"
      ]
    },
    {
      "op": "44",
      "offset": 245,
      "index": 14,
      "value": [
        "「帰ったぞ、セシリア？",
        "/C"
      ]
    },
    {
      "op": "44",
      "offset": 259,
      "index": 15,
      "value": [
        "　留守なのか」",
        "/E"
      ]
    },
    {
      "op": "4A",
      "offset": 270,
      "index": 16,
      "value": [
        "",
        "/E"
      ]
    },
    {
      "op": "44",
      "offset": 272,
      "index": 17,
      "value": [
        "　俺の腕の中で、セシリアがビクッと震えた。",
        "/W"
      ]
    },
    {
      "op": "44",
      "offset": 299,
      "index": 18,
      "value": [
        "　念の為、その口を掌で塞いでおく。",
        "/E"
      ]
    },
    {
      "op": "4A",
      "offset": 323,
      "index": 19,
      "value": [
        "スチュワート",
        "/E"
      ]
    },
    {
      "op": "44",
      "offset": 331,
      "index": 20,
      "value": [
        "「……どうやら、出かけているようだな。",
        "/C"
      ]
    },
    {
      "op": "44",
      "offset": 353,
      "index": 21,
      "value": [
        "　例の日本人青年と一緒なのだろう」",
        "/E"
      ]
    },
    {
      "op": "4A",
      "offset": 380,
      "index": 22,
      "value": [
        "",
        "/E"
      ]
    },
    {
      "op": "44",
      "offset": 382,
      "index": 23,
      "value": [
        "　一人ごちるように、氏が言うのが聞こえた。",
        "/E"
      ]
    },
    {
      "op": "4A",
      "offset": 410,
      "index": 24,
      "value": [
        "スチュワート",
        "/E"
      ]
    },
    {
      "op": "44",
      "offset": 418,
      "index": 25,
      "value": [
        "「まあ、いないものはしょうがない。",
        "/C"
      ]
    },
    {
      "op": "44",
      "offset": 437,
      "index": 26,
      "value": [
        "　今のうちに打ち合わせをしておこう」",
        "/E"
      ]
    },
    {
      "op": "4A",
      "offset": 460,
      "index": 27,
      "value": [
        "",
        "/E"
      ]
    },
    {
      "op": "2E",
      "offset": 462,
      "index": 28,
      "value": [
        "u8:16",
        "u8:3",
        "u8:3",
        "u8:5"
      ]
    },
    {
      "op": "4A",
      "offset": 467,
      "index": 29,
      "value": [
        "ロバート",
        "/E"
      ]
    },
    {
      "op": "44",
      "offset": 473,
      "index": 30,
      "value": [
        "「はい、しかしあの日本人、調子に乗っていませんか？",
        "/C"
      ]
    },
    {
      "op": "44",
      "offset": 506,
      "index": 31,
      "value": [
        "　少し目に余ります」",
        "/E"
      ]
    },
    {
      "op": "4A",
      "offset": 521,
      "index": 32,
      "value": [
        "",
        "/E"
      ]
    },
    {
      "op": "2E",
      "offset": 523,
      "index": 33,
      "value": [
        "u8:15",
        "u8:2",
        "u8:2",
        "u8:5"
      ]
    },
    {
      "op": "4A",
      "offset": 528,
      "index": 34,
      "value": [
        "スチュワート",
        "/E"
      ]
    },
    {
      "op": "44",
      "offset": 536,
      "index": 35,
      "value": [
        "「気になるのかね？",
        "/C"
      ]
    },
    {
      "op": "44",
      "offset": 548,
      "index": 36,
      "value": [
        "　大丈夫、セシリアは君を愛しているよ」",
        "/E"
      ]
    },
    {
      "op": "4A",
      "offset": 574,
      "index": 37,
      "value": [
        "",
        "/E"
      ]
    },
    {
      "op": "2E",
      "offset": 576,
      "index": 38,
      "value": [
        "u8:16",
        "u8:4",
        "u8:3",
        "u8:5"
      ]
    },
    {
      "op": "4A",
      "offset": 581,
      "index": 39,
      "value": [
        "ロバート",
        "/E"
      ]
    },
    {
      "op": "44",
      "offset": 587,
      "index": 40,
      "value": [
        "「い、いえ私は……そんなつもりでは」",
        "/E"
      ]
    },
    {
      "op": "4A",
      "offset": 608,
      "index": 41,
      "value": [
        "",
        "/E"
      ]
    },
    {
      "op": "2E",
      "offset": 610,
      "index": 42,
      "value": [
        "u8:15",
        "u8:1",
        "u8:2",
        "u8:5"
      ]
    },
    {
      "op": "4A",
      "offset": 615,
      "index": 43,
      "value": [
        "スチュワート",
        "/E"
      ]
    },
    {
      "op": "44",
      "offset": 623,
      "index": 44,
      "value": [
        "「いいさ、セシリアのことを思って言ってくれているのはわかる。",
        "/C"
      ]
    },
    {
      "op": "44",
      "offset": 657,
      "index": 45,
      "value": [
        "　それにしても、あの青年には妙に品があるな」",
        "/E"
      ]
    },
    {
      "op": "4A",
      "offset": 685,
      "index": 46,
      "value": [
        "",
        "/E"
      ]
    },
    {
      "op": "2E",
      "offset": 687,
      "index": 47,
      "value": [
        "u8:16",
        "u8:5",
        "u8:3",
        "u8:5"
      ]
    },
    {
      "op": "4A",
      "offset": 692,
      "index": 48,
      "value": [
        "ロバート",
        "/E"
      ]
    },
    {
      "op": "44",
      "offset": 698,
      "index": 49,
      "value": [
        "「は、品ですか？」",
        "/E"
      ]
    },
    {
      "op": "4A",
      "offset": 710,
      "index": 50,
      "value": [
        "",
        "/E"
      ]
    },
    {
      "op": "4A",
      "offset": 712,
      "index": 51,
      "value": [
        "スチュワート",
        "/E"
      ]
    },
    {
      "op": "44",
      "offset": 720,
      "index": 52,
      "value": [
        "「ああ、いい育ちをしているんだろう。",
        "/C"
      ]
    },
    {
      "op": "44",
      "offset": 741,
      "index": 53,
      "value": [
        "　町のチンピラどもとは、明らかに一線を画している」",
        "/E"
      ]
    },
    {
      "op": "4A",
      "offset": 773,
      "index": 54,
      "value": [
        "",
        "/E"
      ]
    },
    {
      "op": "4A",
      "offset": 775,
      "index": 55,
      "value": [
        "スチュワート",
        "/E"
      ]
    },
    {
      "op": "44",
      "offset": 783,
      "index": 56,
      "value": [
        "「そもそも彼は、セシリアの恩人だ。",
        "/C"
      ]
    },
    {
      "op": "44",
      "offset": 805,
      "index": 57,
      "value": [
        "　あいつの無聊を慰めてくれているという事実もある。",
        "/C"
      ]
    },
    {
      "op": "44",
      "offset": 837,
      "index": 58,
      "value": [
        "　ここはひとつ、大人になってはくれないか」",
        "/E"
      ]
    },
    {
      "op": "4A",
      "offset": 862,
      "index": 59,
      "value": [
        "",
        "/E"
      ]
    },
    {
      "op": "4A",
      "offset": 864,
      "index": 60,
      "value": [
        "ロバート",
        "/E"
      ]
    },
    {
      "op": "44",
      "offset": 870,
      "index": 61,
      "value": [
        "「……………………………………………………」",
        "/E"
      ]
    },
    {
      "op": "4A",
      "offset": 894,
      "index": 62,
      "value": [
        "",
        "/E"
      ]
    },
    {
